@model AppCareClinicMed.Models.HISTORICO_PACIENTE

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Paciente
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Paciente.Nome)
        </dd>
        <dt class="col-sm-2">
            Médico
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Medico.Nome)
        </dd>
        <dt class="col-sm-2">
            Data
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Data_historico)
        </dd>
        <dt class="col-sm-2">
            Histórico
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Historico)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Editar</a> |
    <a asp-action="Index">Voltar</a>
</div>
